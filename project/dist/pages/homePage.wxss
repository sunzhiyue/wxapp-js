
.photo {
  display: flex;
  align-content: center;
  justify-content: center;
  height: 100px;
  width: 100px;
}

.section {
  display: flex;
  align-content: center;
  border: 1px solid grey;
  border-radius: 5px;
  background-color: white;
  justify-content: center;
}

.slide-image {
  width: 100%;
  height: 12.5rem;
}

.background {
  background-color: #cdcdcd;
  padding: 10px;
}

.search {
  height: 20px;
  width: 20px;
  display: flex;
  align-content: center;
  justify-content: center;
  margin-top: 1px;
}
.show {
  width: 100%;
  height: 200rpx;
}
